{
  "name": "cozy-konnector-generali",
  "version": "1.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-generali.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-konnector-dev",
    "standalone": "cozy-konnector-standalone",
    "pretest": "npm run clean",
    "test": "jest",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-generali.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.15.2",
    "request-promise": "4.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "copy-webpack-plugin": "5.0.2",
    "cozy-app-publish": "0.15.0",
    "cozy-jobs-cli": "1.8.3",
    "eslint": "5.16.0",
    "eslint-config-cozy-app": "1.1.12",
    "eslint-plugin-prettier": "3.0.1",
    "git-directory-deploy": "1.5.1",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "konitor": "0.10.2",
    "svgo": "1.2.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  }
}
