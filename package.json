{
  "name": "cozy-konnector-generali",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-generali.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "renovate": {
    "extends": ["config:base"],
    "patch": {
      "automerge": true
    }
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "eslintIgnore": ["build"],
  "scripts": {
    "start": "node src/index.js",
    "dev": "cozy-run-dev src/index.js",
    "standalone": "cozy-run-standalone",
    "build": "webpack",
    "test": "jest",
    "format": "prettier --write '**/*.{js,json}'",
    "precommit": "eslint .",
    "lint": "eslint .",
    "deploy":
      "git-directory-deploy --directory build/ --branch build --repo=https://github.com/konnectors/cozy-konnector-generali.git",
    "deploy:prod":
      "git-directory-deploy --directory build/ --branch latest --repo=https://github.com/konnectors/cozy-konnector-generali.git"
  },
  "dependencies": {
    "cozy-jobs-cli": "^1.0.3",
    "cozy-konnector-libs": "^3.7.2",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "copy-webpack-plugin": "4.5.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13"
  }
}
